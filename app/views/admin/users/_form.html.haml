%h3= "Editing user #{@user.email}"
- semantic_form_for ["admin",@user] do |form|
  - form.inputs do
    = form.input :name
    = form.input :password
    = form.input :password_confirmation
    = form.input :phone
    = form.input :company
    = form.input :address1
    = form.input :address1
    = form.input :city
    = form.input :state
    = form.input :zip
    
  - form.buttons do
    = form.commit_button 'Save',:button_html => { :class => "submit" }